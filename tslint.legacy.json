{
  "extends": [
    "./tslint.json"
  ],
  "rules": {
    "quotemark": [
      true,
      "double",
      "avoid-escape",
      "jsx-double"
    ],
    "indent": [
      true,
      "spaces",
      4
    ],
    "ter-indent": [
      true,
      4,
      {
        "SwitchCase": 1
      }
    ]
  }
}
